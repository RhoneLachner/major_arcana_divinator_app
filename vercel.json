{
    "headers": [
        {
            "source": "/assets/font/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ],
    "routes": [
        {
            "src": "/assets/font/(.*)",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        }
    ]
}
